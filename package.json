    "@types/jest": "^27.0.2",
    "@types/node": "^15.12.2",
    "ctl-scripts": "^0.3.4",
    "cz-conventional-changelog": "3.3.0",
    "jest": "^27.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.0",
    "rollup-plugin-typescript2": "^0.31.0",
    "ts-jest": "^27.0.7",
    "tslib": "^2.3.1",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "fs-extra": "^10.0.0",
    "outvariant": "^1.2.1"
  },
  "config": {
